mat_1 <- matrix(1:9,
              ncol = 3,
              dimnames = list(paste0("cell", 1:3), paste0("m", 1:3))
)

mat_2 <- matrix(9:1,
              ncol = 3,
              dimnames = list(paste0("cell", 1:3), paste0("m", 1:3))
)
# Rows are: c(1,4,7), c(2,5,8), c(3,6,9) -> maxima: 7, 8, 9
mat_1_max <- parallel_row_max(mat_1)
mat_2_min <- parallel_row_min(mat_2)
parallel_difference(mat_1_max, mat_2_min)
